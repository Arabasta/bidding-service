{
  "app": {
    "name": "bidding command service",
    "environment": "development",
    "version": "1.0.0"
  },
  "network": {
    "Port": 8080,
    "IsLocalHost": false,
    "ssl": {
      "enabled": false,
      "certFile": "",
      "keyFile": "",
      "caFile": ""
    }
  },
  "Cors": {
    "isEnabled": true,
    "allowOrigins": ["http://localhost:8080"],
    "allowMethods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
    "allowHeaders": ["Authorization", "Content-Type", "X-Requested-With"],
    "exposeHeaders": ["ETag"],
    "allowCredentials": true,
    "allowMaxAge": 600
  },
  "Swagger": {
    "IsEnabled": true,
    "Title": "Bidding Command Service API",
    "OpenApiName": "openapi"
  },
  "Pprof": {
    "IsEnabled": true,
    "Port": 6060
  },
  "Logger": {
    "level": "debug",
    "format": "console",
    "output": "stdout",
    "filePath": "./log/bid.log"
  },
  "Postgres": {
    "user": "postgres",
    "port": 5432,
    "dbName": "bidding",
    "params": "",
    "maxIdleConns": 2,
    "maxOpenConns": 10,
    "connMaxLifetimeMinutes": 30,
    "sslEnabled": false,
    "sslMode": "disable",
    "logLevel": 4
  },
  "redis": {
    "Addr": "redis",
    "port": 6379,
    "poolSize": 10,
    "password": ""
  },
  "kafka": {
    "brokers": ["kafka:9092"],
    "bidsTopic": "bids.placed",
    "openedTopic": "auction.opened",
    "closedTopic": "auction.closed",
    "groupId": "bidding-meta-sync",
    "clientId": "bidding-service",
    "acks": "all",
    "compression": "snappy"
  }
}
